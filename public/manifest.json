    {
      "name": "Start in dark mode",
      "short_name": "Dark mode",
      "description": "Start W PWA Example in dark mode",
      "url": "/?source=pwa&mode=night",
      "icons": [
        {
          "src": "/images/logo.svg",
          "sizes": "150x150"
        }
      ]
    },
    {
      "name": "Start in light mode",
      "short_name": "Light mode",
      "description": "Start W PWA Example in light mode",
      "url": "/?source=pwa&mode=day",
      "icons": [
